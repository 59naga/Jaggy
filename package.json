{
  "name": "jaggy",
  "main": "jaggy",
  "bin": "jaggy",
  "description": "is Converting to SVG by pixels",
  "version": "0.1.3",

  "scripts": {
    "build": "browserify lib/jaggy.coffee -r get-pixels -r gify-parse -t coffeeify > public/jaggy.browser.js",

    "prestart": "onefile --json --output public/pkgs",
    "start": "cd public && open http://localhost:8000 && python -m SimpleHTTPServer",

    "convert": "jaggy public -o hogekosan -g 2",

    "test": "jasminetea test --verbose --cover --report",
    "posttest": "rm public/*.svg",

    "prepublish": "npm run build",
    "publish": "npm publish"
  },

  "dependencies": {
    "browserify": "^9.0.3",
    "coffee-script": "^1.9.0",
    "commander": "^2.6.0",
    "dom-lite": "^0.4.0",
    "get-pixels": "^3.1.0",
    "gify-parse": "^1.0.4",
    "gulp-util": "^3.0.3",
    "mime": "^1.2.11",
    "through2": "^0.6.3"
  },
  "devDependencies": {
    "browserify": "^8.1.3",
    "coffeeify": "^1.0.0",
    "jasminetea": "^0.1.27"
  },
  "keywords": [
    "pixelart",
    "get-pixels",
    "cross-platform",
    "gulpplugin",
    "gif"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/59naga/jaggy.git"
  },
  "bugs": {
    "url": "https://github.com/59naga/jaggy/issues"
  },
  "homepage": "https://github.com/59naga/jaggy",
  "author": "59naga <i59naga@icloud.com> (http://berabou.me/)",
  "license": "MIT"
}
